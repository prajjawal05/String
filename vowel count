#include <stdio.h>
#include <stdlib.h>
#include<string.h>
int main()
{
    int i,j=0,l;
    char s[1000];
    gets(s);
    l=strlen(s);
    for(i=0;i<l;i++)
    {
        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U')
            j++;
    }
    printf("%d", j);
    return 0;
}
