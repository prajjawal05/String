#include<string.h>
#include<stdlib.h>
#include<stdio.h>
void main()
{	
	char s1[100], s2[100],c;
	printf("Enter s1 and s2");
	scanf("%s %s",s1,s2);
	int len,i,j;
	len=strlen(s2);
        i=0,j=len-1;
		while(i<=j&&i!=j)
		{
			c=s2[i];
			s2[i]=s2[j];
			s2[j]=c;
			i++;
			j--;
		}
	printf("%s%s",s1,s2);
}
		
